CELERY_WORKER_DIR="/usr/share/mediagoblin"
CELERY_WORKER_CMD="${CELERY_WORKER_DIR}/bin/celery"
CELERY_WORKER_PID="/run/mediagoblin/celery-worker.pid"
CELERY_WORKER_LOG="/var/log/mediagoblin/celery-worker.log"
CELERY_WORKER_USER="mediagoblin"
CELERY_WORKER_GROUP="mediagoblin"
CELERY_WORKER_ARGS="worker --logfile=${CELERY_WORKER_LOG} --pidfile=${CELERY_WORKER_PID} -D"

